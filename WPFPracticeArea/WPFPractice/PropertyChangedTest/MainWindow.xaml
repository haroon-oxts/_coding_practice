<Window x:Class="PropertyChangedTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PropertyChangedTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" FontSize="20" Content="{Binding DudeOne.Name}"></Label>
        <Label Grid.Row="1" FontSize="20" Content="{Binding DudeOne.AssSize}"></Label>
        <Label Grid.Row="2" FontSize="20" Content="{Binding DudeOne.Pizza.NumberOfPepperoni}"></Label>
        <Label Grid.Row="3" FontSize="20" Content="{Binding DudeOne.Pizza.Name}"></Label>
        <Button Grid.Row="4" Click="Button_Click" ></Button>
    </Grid>
</Window>
